# platform :ios, '9.0'

target 'TwitterMock' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  
  pod 'RxSwift'
  pod 'RxCocoa'
  pod 'RealmSwift'
  pod 'RxRealm'
  pod 'KeychainAccess'
  pod 'Swinject'
  pod 'SwinjectStoryboard'
  pod 'SDWebImage'
  pod 'SwiftyJSON'
  
  target 'TwitterMockTests' do
    inherit! :search_paths
    use_frameworks!
    
  end
  
  target 'TwitterMockUITests' do
    inherit! :search_paths
    use_frameworks!
    
    pod 'RxSwift'
    pod 'RxCocoa'
    pod 'RealmSwift'
    pod 'RxRealm'
    pod 'KeychainAccess'
    pod 'Swinject'
    pod 'SwinjectStoryboard'
    pod 'SDWebImage'
    pod 'SwiftyJSON'
    
  end
  
end
